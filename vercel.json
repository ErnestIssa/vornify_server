{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "STRIPE_PUBLIC_KEY": "@stripe_public_key"
  }
} 